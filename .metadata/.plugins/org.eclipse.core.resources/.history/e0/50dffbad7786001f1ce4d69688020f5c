package entity;

public class Doctor {
	
	static long id = 2000;
	String doctorId;
	String doctorName;
	String specialization;
	String conctacNumber;
	boolean isAvailable;
	/**
	 * @param doctorName
	 * @param specialization
	 * @param conctacNumber
	 */
	public Doctor(String doctorName, String specialization, String conctacNumber) {
		super();
		this.doctorId = "D" + ++id;
		this.doctorName = doctorName;
		this.specialization = specialization;
		this.conctacNumber = conctacNumber;
		this.isAvailable = true;
	}
	public String getDoctorName() {
		return doctorName;
	}
	public void setDoctorName(String doctorName) {
		this.doctorName = doctorName;
	}
	public String getSpecialization() {
		return specialization;
	}
	public void setSpecialization(String specialization) {
		this.specialization = specialization;
	}
	public String getConctacNumber() {
		return conctacNumber;
	}
	public void setConctacNumber(String conctacNumber) {
		this.conctacNumber = conctacNumber;
	}
	public String getDoctorId() {
		return doctorId;
	}
	public boolean isAvailable() {
		return isAvailable;
	}
	@Override
	public String toString() {
		return "Doctor [doctorId=" + doctorId + ", doctorName=" + doctorName + ", specialization=" + specialization
				+ ", conctacNumber=" + conctacNumber + ", isAvailable=" + isAvailable + "]";
	}
	
	

}
